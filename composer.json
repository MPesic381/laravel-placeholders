{
    "name": "mpesic381/laravel-placeholders",
    "description": "Find and Replace placeholder strings in Laravel 8",
    "keywords": [
        "mpesic381",
        "laravel-placeholders"
    ],
    "homepage": "https://github.com/mpesic381/laravel-placeholders",
    "version": "1.0.0",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Miomir Pesic",
            "email": "pesicmiomir@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0|^8.1|^8.2",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "MPesic381\\Placeholders\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MPesic381\\Placeholders\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "MPesic381\\Placeholders\\PlaceholdersServiceProvider"
            ],
            "aliases": {
                "Placeholders": "Placeholders"
            }
        }
    }
}
